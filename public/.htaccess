<IfModule mod_rewrite.c>
    Options -Multiviews
    RewriteEngine On
    
    # Pastikan RewriteBase sesuai nama folder di htdocs
    RewriteBase /Code/Project_ICLabs/iclabs_v2/public/
    
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
</IfModule>
